Alphabet Pattern Printing
const readline = require('readline').createInterface({
  input: process.stdin,
  output: process.stdout
});

readline.question('', n => {
  // Write your code here
  parseInt(n)
  for (let i = 0; i < n; i++) {
    let row = "";
    let char = String.fromCharCode(65 + i); 
    for (let j = 0; j <= i; j++) {
      row += char;
    }
    console.log(row);
  }
  
  readline.close();
});
